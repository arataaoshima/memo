<div class="container">
<h2>Create New Post</h2>
<% breadcrumb :new_post %>
<%= form_tag("/post/create") do %>
<form>
<div class="form-group">
    <label>Article Title</label>
    <input name="post" class="form-control" placeholder="Title of the Article">
  </div>
 <div class="form-group">
<label>Description</label>
<textarea name="post" class="form-control" rows="8"></textarea>
  </div>
<p>Click Here to submit</p>
<input type="submit">
</form>
<% end %>
</div>